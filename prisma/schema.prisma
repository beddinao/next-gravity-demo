generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Object {
  id  Int   @id @default(autoincrement())
  name  String
  radius  Int
  mass  Int
  x Int
  y Int
  vx  Int
  vy  Int
  ax  Int
  ay  Int
}